<script type="text/javascript">
    function setSupplier() {
     var desiredOption = $("#el_supplier").val();
     var hasOption = $('#gas_supplier option[value="' + desiredOption + '"]');
     if (hasOption.length == 0)
     {
         $('#gas_supplier')[0].selectedIndex = 0;
     }
     else
         $('#gas_supplier').val(desiredOption);
     $('#gas_product')[0].selectedIndex = 0;
     $('#el_product')[0].selectedIndex = 0;
    }
    function setProduct() {
     var desiredOption = $("#el_product").val();
     var hasOption = $('#gas_product option[value="' + desiredOption + '"]');
     if (hasOption.length == 0)
     {
         $('#gas_product')[0].selectedIndex = 0;
     }
     else
         $('#gas_product').val(desiredOption);
    }

      <meta property="og:image:width" content="1024" />
      <meta property="og:image:height" content="512" />
      <!-- Twitter -->
      <meta name="twitter:card" content="summary" />
      <meta name="twitter:site" content="@contractfit" />
      <meta name="twitter:title" content="<%= sess.brand  %> - <%= gt.gettext("Verander van energieleverancier in 60 seconden"); %>" />

               <form method="POST" action="/<%=sess.site_lang%>/manual" id="manual-form" novalidate>
                  <div class="cf-column-interior" style="display:;">
                     <div class="result-content">
                        <input type="email" <%= body.email? 'ng-init="email=\'' + body.email + '\'"': "" %> id="email" name="email" ng-model="email" value="<%= helpers.set_value(body.email);%>" placeholder='<%= gt.gettext("email") %>' autofocus/>
                        <input type="text" <%= body.zip_code? 'ng-init="zip_code=\'' + body.zip_code + '\'"': "" %> id="zip_code" name="zip_code" ng-model="zip_code" value="<%= helpers.set_value(body.zip_code) %>" placeholder='<%= gt.gettext("postcode") %>' />
                        <br>
                        <span style="display:inline;">
                        <%= gt.gettext("U bent") %>: </span>
                        <span style="display:inline;" ng-show="client_type=='consumer'">
                        <%= gt.gettext("Particulier") %>
                        </span>
                        <span style="display:inline;" ng-show="client_type=='sme'">
                        <%= gt.gettext("Professioneel") %>
                        </span>
                        <a href="" ng-show="client_type && !unfold_client_type" ng-click="unfold_client_type=!unfold_client_type">

       document.getElementById('zip_code').focus();
   }
   
   function getGewest(zipcode) {
       if (/^[0-9]{4}$/.test(zipcode))
           return "NONE";
       var code = parseInt(zipcode);
       if (code < 1300)
           return "BXL";
       if (code < 1500)
           return "WL";
       if (code < 4000)
           return "VL";
       if (code < 8000)
           return "WL";
       return "VL";
   }
</script>
<script>
   $(document).ready(function () {
       function getCookie(name) {
           var value = "; " + document.cookie;
           var parts = value.split("; " + name + "=");
           if (parts.length == 2) return parts.pop().split(";").shift();
       }
       var formData = getCookie("formdata");
       if (formData !== undefined) {
           $("#manual-form").deserialize(formData);
       }
       $("#manual-form").on("focusout", function () {
           var d = new Date();
           //one day
           d.setTime(d.getTime() + (24 * 60 * 60 * 1000));
           document.cookie = "formdata=" + $(this).serialize() + ";expires=" + d.toUTCString() +
               ";path=" + location.pathname + "/;";
       });
   });
</script>

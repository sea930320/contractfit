                      // validate the comment form when it is submitted
                      $("#manual-form").validate({
                          errorPlacement: function (label, element) {
                              console.log(element);
                              if (element[0].type == 'radio' ||  element[0].type ==
                                  'checkbox')
                                  label.insertBefore(element);
                              else
                                  label.insertAfter(element);
                          },
                          wrapper: 'span',
                          rules: {
                              email: {
                                  required: true,
                                  email: true
                              },
                              zip_code: {
                                  required: true,

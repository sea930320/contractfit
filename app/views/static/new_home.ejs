                browseIcon: "<i class=\"icon-folder-open\"></i> ",
                removeClass: "btn btn-danger",
                removeLabel: "",
                removeIcon: "<i class=\"icon-trash\"></i> ",
                uploadClass: "btn btn-info",
                uploadLabel: "Upload",
                uploadIcon: "<i class=\"icon-upload\"></i> ",
                allowedFileExtensions: ["*"],
            });

            $(".form-control.file-caption.kv-fileinput-caption").click(function (event) {
                $(event.target.nextElementSibling).find("input[type=file]").click();
            });

            $("#pictureuploadfile").on('change', function (event) {
                var form = document.getElementById("pictureuploadform");
                if (form !== null) {
                    event.preventDefault();
                    $('#home_modal_picture').modal('hide');
                    $('#myModal').modal('show');
                    form.submit();
                    return;
                }
            });

            new Clipboard('.copy-button');
        });

        function isValidEmail(email) {
            var filter = /^[\w\-\.\+]+\@[a-zA-Z0-9\.\-]+\.[a-zA-z0-9]{2,4}$/;
            return email.length != 0 && filter.test(email);
        }

        function home_modal_upload_button() {
            if ($('#txtMail').val() && isValidEmail($('#txtMail').val())) {
                document.getElementById('upload').click();
            } else {
                $('#txtMail').addClass("error");
            }
        }

        function home_modal_picture_button() {
            if ($('#pictureuploademail').val() && isValidEmail($('#pictureuploademail').val())) {
                document.getElementById('pictureuploadfile').click();
            } else {
                $('#pictureuploademail').addClass("error");
            }
        }
    </script>
    <div class="modal fade" tabindex="-1" role="dialog" aria-hidden="true" id="home_modal_upload">
        <div class="modal-dialog">
            <div class="modal-body">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <!--<h4 class="modal-title" style="text-align: center"><%= gt.gettext("Uploaden van een factuur of scan") %></h4>-->
                        <h4 class="modal-title" style="text-align: center">
                            <%= gt.gettext("Ik upload mijn factuur") %>
                        </h4>
                    </div>
                    <div class="modal-body center">
                        <div class="feedback-output">
                            <%
                                if(sess.form_errors && sess.form_error_type && sess.form_error_type == "upload"){
                                    feedback = sess.form_errors;
                                    feedback.forEach(function(message) {
                            %>
                                <%- '<small ' + (feedback['success'] ? 'class="success"' : 'class="error"') + '>' + message +  ' </small>';
                                    %>
                            <%
                                })
                                    sess.form_errors = null;
                                }
                                %>
                        </div>
                        <form id="upload-form" name="template-contactform" method="post" novalidate action="upload_invoice">
                            <input type="hidden" name="pdfInvoice" value="true" />
                            <div style="margin-top:20px;width:100%;text-align:left;">
                                <span class="left" style="width:90%;">
                                    <%= gt.gettext("Uw email adres") %>
                                </span>
                                <input type="email" required class="upload-email form-control" name="email" placeholder="<%= gt.gettext("Email adres") %>" id="txtMail"
                                    style="display: inline; width:inherit;" <%=(sess.email ?"value = '" + sess.email + "'":"")%>
                                />
                            </div>
                            <div style="margin-top:20px;text-align:left;">
                                <span class="left" style="width:90%;">
                                    <%= gt.gettext("Uw factuur") %>
                                </span>
                                <input name="fileToUpload[]" multiple required class="file-upload-wrapper" id="upload" type="file" accept="*" class="file"
                                    data-allowed-file-extensions='[]'>
                            </div>
                            <div class="file-upload topmargin">
                                <button class="btn btn-lg btn-danger btn-block nomargin" value="submit" id="upload-button" type="submit">
                                    <i class="icon-share2"></i>&nbsp;&nbsp;
                                    <%= gt.gettext("Verstuur") %>
                                </button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>        
    <div class="modal fade" tabindex="-1" role="dialog" aria-hidden="true" id="home_modal_picture">
        <div class="modal-dialog">
            <div class="modal-body">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <!-- <h4 class="modal-title" style="text-align: center"><%= gt.gettext("Foto maken van een papieren factuur") %></h4> -->
                        <h4 class="modal-title" style="text-align: center">
                            <%= gt.gettext("Ik maak een foto van mijn papieren factuur") %>
                        </h4>
                    </div>
                    <div class="modal-body center">
                        <div class="feedback-output">
                            <%
                                if(sess.form_errors && sess.form_error_type && sess.form_error_type == "picture"){
                                    feedback = sess.form_errors;
                                    feedback.forEach(function(message) {
                            %>
                                <%- '<small ' + (feedback['success'] ? 'class="success"' : 'class="error"') + '>' + message +  ' </small>' %>
                            <%
                                    })
                                    sess.form_errors = null;
                                }
                                %>
                        </div>
                        <form class="clearfix" method="post" novalidate action="share_by_picture" enctype="multipart/form-data" id="pictureuploadform">
                            <div style="margin-top:20px;width:100%">
                                <input type="email" required class="upload-email form-control" name="email" placeholder="<%= gt.gettext(" Uw email adres ") %>*" id="pictureuploademail"
                                    style="display: inline; width:inherit;" <%=(sess.email ? "value = '" + sess.email + "'":"") %>
                                />
                            </div>
                            <div class="hidden">
                                <input name="fileToUpload[]" required class="file" type="file" accept="image/*" capture="camera" id="pictureuploadfile">
                            </div>
                            <div class="center nobg topmargin">
                                <button class="btn btn-danger btn-circle" type="button" onclick="home_modal_picture_button()">
                                    <i class="icon-line-camera"></i>&nbsp;&nbsp;
                                    <%= gt.gettext("Maak een foto") %>
                                </button>
                            </div>
                        </form>
                        <p class="topmargin">
                            <%= gt.gettext("Kies een foto of van op jouw toestel of maak een nieuwe foto met de camera. Hou de camera ongeveer 30cm recht boven de eerste pagina van de factuur.") %>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" tabindex="-1" role="dialog" aria-hidden="true" id="home_modal_email">
        <div class="modal-dialog">
            <div class="modal-body">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <h4 class="modal-title" style="text-align: center">
                            <%= gt.gettext("Ik mail mijn factuur") %>
                        </h4>
                    </div>
                    <div class="modal-body center">
                        <p class="bottommargin">
                            <%= gt.gettext("Stuur je factuur of de email van jouw energieleverancier door naar dit adres.") %>
                        </p>
                        <div class="input-group input-group-md" style="margin-bottom: 10px; width: 100%;">
                            <a href="mailto:<%= vsprintf(gt.gettext(" factuur@%s "),[sess.brand]); %>">
                                <h3 id="copyTarget" data-clipboard-target="#copyTarget" data-trigger="focus">
                                    <%= vsprintf(gt.gettext("factuur@%s"),[sess.brand]); %>
                                </h3>
                            </a>
                            <div class="topmargin">
                                <button onclick="this.children[1].textContent='<%= gt.gettext("Copied!") %>'" class="copy-button btn btn-danger" type="button" data-clipboard-demo=""
                                    data-clipboard-target="#copyTarget">
                                    <i class="icon-copy"></i>&nbsp;&nbsp;
                                    <span>
                                        <%= gt.gettext("Copy") %>
                                    </span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <%
            if(sess.form_error_type) {
    %>
                <%- '<script>$(document).on("ready", function() {$("#home_modal_' + sess.form_error_type + '").modal("show")})</script>' %>
                
    <%
                sess.form_error_type = null;
            }
    %>
        <%- include("explainer") %>
</section>

<%- include('init_header'); %>
<header id="header" class="transparent-header dark  sticky" data-sticky-class="not-dark">
    <%- include('header'); %>
</header>
<section id="slider" class="slider-parallax">
    <div class="clearfix full-screen section nomargin nopadding dark " style="background: url('/images/videos/energy.png') center center no-repeat; background-size: cover;">
        <div class="slider-parallax-inner">
            <div class="container vertical-middle center clearfix">
                <div class="emphasis-title nomargin" data-animate="fadeInUp">
                    <!-- <h2 data-caption-animate="fadeInUp"><%= gt.gettext("Energie: leveranciers vergelijken, kiezen en overstappen") %></h2> -->
                    <h2 data-caption-animate="fadeInUp">
                        <%= gt.gettext("Vergelijk, kies en bespaar op gas en elektriciteit") %>
                    </h2>
                    <!-- <p data-caption-animate="fadeInUp" data-caption-delay="200"><%= gt.gettext("Goeie keuzes zijn belangrijk. Ontvang in minder dan 60 seconden een gratis analyse op basis van jouw factuur en hulp bij de overstap!") %></p> -->
                    <h4 data-caption-animate="fadeInUp" data-caption-delay="200">
                        <%= gt.gettext("Goeie keuzes zijn belangrijk, ook voor energie. Laat nu in 60 seconden je factuur analyseren en krijg hulp bij de overstap. Gratis en zonder gedoe.") %>
                    </h4>
                    <!-- <a data-animate="fadeInUp" data-delay="600" href="#section-movie" class="button button-border button-light button-rounded button-large noleftmargin nobottommargin" style="margin-top: 30px;"><%= gt.gettext("Meer lezen") %></a> -->
                    <a data-animate="fadeInUp" data-delay="600" href="#section-movie" class="button button-border button-light button-rounded button-large noleftmargin nobottommargin makesmalleronmobile"
                        style="margin-top: 30px;">
                        <%= gt.gettext("Benieuwd hoe dit kan?") %>
                    </a>
                    <!-- <a data-animate="fadeInUp" data-delay="800" href="#section-form" class="button button-3d button-red button-large nobottommargin" style="margin: 30px 0 0 10px;"><%= gt.gettext("Start nu") %></a> -->
                    <a data-animate="fadeInUp" data-delay="800" href="#section-form" class="button button-3d button-red button-large nobottommargin makesmalleronmobile"
                        style="margin: 30px 0 0 0px;">
                        <%= gt.gettext("Ik ken het al, start nu !") %>
                    </a>
                </div>
            </div>
            <div id="movie" class="yt-bg-player top-cart" data-quality="hd1080" data-video="https://youtu.be/Mio9NWbnd5Q" data-opacity="50"
                data-autoplay="true" style="width: 100%;z-index: 0">
            </div>
            <div class="video-overlay" style="background-color: rgba(0,0,0,0.55);z-index:1"></div>
            <a href="#section-movie" data-scrollto="#section-movie" data-offset="0" class="dark one-page-arrow">
